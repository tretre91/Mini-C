void print_fizz() {
  putchar(70);
  putchar(105);
  putchar(122);
  putchar(122);
}

void print_buzz() {
  putchar(66);
  putchar(117);
  putchar(122);
  putchar(122);
}

int mod(int a, int b) {
  while(b <= a) {
    a = a - b;
  }
  return a;
}

void print_int(int i) {
  int div = i / 10;
  int left = i - div * 10;
  if (div >= 1) {
    print_int(div);
  } else {}
  putchar(left + 48);
}

void fizzbuzz() {
  int i = 1;
  bool fizz;
  bool buzz;
  bool changed;
  while (i <= 100) {
    changed = false;
    
    if (mod(i, 3) == 0) {
      print_fizz();
      changed = true;
    } else {}

    if (mod(i, 5) == 0) {
      print_buzz();
      changed = true;
    } else{}

    if (changed == false) {
      print_int(i);
    } else {}
    
    putchar(10);
    i = i + 1;
  }
}

int main() {
  fizzbuzz();
  return 0;
}
